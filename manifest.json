{
    "manifest_version": 3,
    "name": "Fast Tab",
    "version": "1.0",
    "permissions": [
        "tabs"
    ],
    "action": {
        "default_popup": "src/popup.html"
    },
    "background": {
        "service_worker": "src/service_worker.js"
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Alt+Shift+P",
                "linux": "Alt+Shift+P",
                "windows": "Alt+Shift+P",
                "mac": "Alt+Shift+P"
            },
            "description": "Open the extension popup"
        }
    }
}